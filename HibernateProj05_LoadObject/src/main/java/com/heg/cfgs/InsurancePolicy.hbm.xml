
<hibernate-mapping>
    
    <!-- Mapping the Product class to the "product" table -->
    <class name="com.heg.entity.InsurancePolicy" table="insurancepolicy">
        

        <id name="policyId" column="policyId"> </id>
        
        
        <property name="policyName" column="policyName" />
        
        
        <property name="tenure" column="tenure" />
        
        
        <property name="company" column="company" />
        <property name="type" column="type" />

        
    </class>

</hibernate-mapping>
